{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@logchimp/types",
  "version": "0.8.0-beta.2",
  "private": true,
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
