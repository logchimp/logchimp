    "binary-extensions": "^2.3.0",
    "brace-expansion": "^1.1.12",
    "braces": "^3.0.3",
    "chokidar": "^3.6.0",
    "concat-map": "^0.0.1",
    "debug": "^4.4.1",
    "fill-range": "^7.1.1",
