{
  "name": "logchimp",
  "description": "A open-source software to track your customers feedback",
  "version": "0.7.0-beta.4",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/logchimp/logchimp.git"
  },
