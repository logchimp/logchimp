{
	"name": "logchimp",
	"description": "A open-source software to track user feedback",
	"version": "0.5.0",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/logchimp/logchimp.git"
	},
	"bugs": {
		"url": "https://github.com/logchimp/logchimp/issues"
	},
	"scripts": {
		"frontend:dev": "cd frontend && vue-cli-service serve",
		"frontend:build": "cd frontend && vue-cli-service build",
		"frontend:stylelint": "stylelint --syntax sass './frontend/src/styles/**/*.sass'",
		"frontend:test": "jest --config ./frontend/jest.config.js",
		"db:migrate": "knex --knexfile ./server/database/config.js migrate:latest",
		"server:dev": "cd server && nodemon app.js",
		"server:start": "cd server && node app.js",
		"server:test": "jest --config ./server/jest.config.js",
		"eslint:fix": "eslint --fix -c ./.eslintrc.js ./"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"express-rate-limit": "^5.2.3",
		"fs-extra": "^9.0.1",
		"html-to-text": "^6.0.0",
		"jsonwebtoken": "^8.5.1",
		"knex": "^0.21.15",
		"lodash": "^4.17.20",
		"md5": "^2.3.0",
		"multer": "^1.4.2",
		"nanoid": "^3.1.20",
		"node-sass": "^5.0.0",
		"nodemailer": "^6.4.17",
		"pg": "^8.5.1",
		"sass-loader": "^10.1.0",
		"uuid": "^8.3.2",
		"vue": "^2.6.12",
		"vue-gtag": "^1.10.0",
		"vue-infinite-loading": "^2.4.5",
		"vue-meta": "^2.4.0",
		"vue-moment": "^4.1.0",
		"vue-router": "^3.4.9",
		"vuex": "^3.6.0",
		"winston": "^3.3.3"
	},
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/preset-env": "^7.12.11",
		"@vue/cli-plugin-babel": "^4.5.9",
		"@vue/cli-plugin-eslint": "^4.5.7",
		"@vue/cli-service": "^4.5.9",
		"@vue/test-utils": "^1.1.2",
		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^10.0.3",
		"babel-jest": "^26.6.3",
		"babel-plugin-transform-require-context": "^0.1.1",
		"eslint": "^7.16.0",
		"eslint-config-prettier": "^7.1.0",
		"eslint-plugin-jest": "^24.1.3",
		"eslint-plugin-prettier": "^3.3.0",
		"eslint-plugin-vue": "^7.4.0",
		"jest": "^26.6.3",
		"nodemon": "^2.0.7",
		"omgopass": "^3.2.1",
		"regenerator-runtime": "^0.13.7",
		"stylelint": "^13.8.0",
		"stylelint-scss": "^3.18.0",
		"vue-jest": "^3.0.7",
		"vue-template-compiler": "^2.6.12"
	},
	"keywords": [
		"logchimp",
		"feedback",
		"open-source",
		"codecarrot"
	],
	"engines": {
		"node": ">=12.18.4",
		"yarn": ">=1.22"
	}
}
